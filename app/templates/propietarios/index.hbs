<ul class="collection">
    <li class="collection-header">
      <h5>
        <i class="material-icons" title="Usuarios">group</i>Cooperativistas
        <span class="secondary-content">
          {{#link-to 'propietarios.new' class='green'}}<i class="material-icons right">add</i>{{/link-to}}
          {{#link-to 'propietarios'}}<i class="material-icons right">refresh</i>{{/link-to}}
          <i class="material-icons">group</i>{{propietarios.length}}
        </span>
      </h5>
    </li>
</ul>

<ul class="collection">
{{#each propietarios as |propietario|}}
<li>
  {{#link-to 'propietarios.edit' propietario.id}}<i class="material-icons">edit</i>{{/link-to}}
  <a class="red" {{action 'deleteModel' propietario}}><i class="material-icons">delete</i></a>
  {{propietario.name}} {{propietario.surname1}} {{propietario.surname2}}
</li>
{{/each}}
</ul>
{{outlet}}